{
    "name": "verbb/kint",
    "description": "Adds Kint, an in-app PHP debugger for use in Twig and PHP.",
    "type": "craft-plugin",
    "version": "4.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "kint"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/kint/issues?state=open",
        "source": "https://github.com/verbb/kint",
        "docs": "https://github.com/verbb/kint",
        "rss": "https://github.com/verbb/kint/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        },
        {
            "name": "Mildly Geeky, Inc.",
            "homepage": "https://mildlygeeky.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^5.0.0-beta.1",
        "kint-php/kint": "^4.0",
        "kint-php/kint-twig": "^3.0",
        "verbb/base": "^3.0.0-beta.1"
    },
    "autoload": {
        "psr-4": {
            "verbb\\kint\\": "src/"
        }
    },
    "extra": {
        "name": "Kint",
        "handle": "kint",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/kint/craft-5/CHANGELOG.md",
        "class": "verbb\\kint\\Kint"
    }
}
